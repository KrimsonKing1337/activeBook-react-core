{
  "name": "activebook-core",
  "version": "0.0.32",
  "description": "",
  "main": "./",
  "scripts": {
    "make": "node ./scripts/make.js",
    "clean": "rimraf dist tgz lib lib-esm",
    "ts": "tsc && tsc -m es6 --outDir lib-esm",
    "copy-files": "copyfiles -u 1 -s src/**/*.* lib/src/ -e src/**/*.ts -e src/**/*.tsx",
    "packaging": "node ./scripts/tgzPrepare.js && npm pack --pack-destination ./tgz",
    "up": "npm version patch --git-tag-version false --commit-hooks false",
    "scss": "node-sass ./src --output ./src",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "node ./scripts/postinstall.js",
    "lint-staged": "lint-staged",
    "lint:ts": "eslint \"./src/**/*.{ts,tsx}\" --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint --fix"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-brands-svg-icons": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@reduxjs/toolkit": "^1.9.5",
    "axios": "^0.21.1",
    "classnames": "^2.3.2",
    "encrypt-storage": "^2.12.16",
    "hammerjs": "^2.0.8",
    "howler": "^2.2.3",
    "modern-css-reset": "^1.4.0",
    "moment": "^2.29.4",
    "nanoid": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^7.2.9",
    "react-router": "^6.15.0",
    "react-router-dom": "^6.15.0",
    "react-slider": "^2.0.6",
    "react-toastify": "^9.1.3",
    "redux-first-history": "^5.1.1",
    "redux-saga": "^1.2.3"
  },
  "devDependencies": {
    "@types/classnames": "^2.3.1",
    "@types/hammerjs": "^2.0.41",
    "@types/howler": "^2.2.8",
    "@types/node": "^20.5.6",
    "@types/react": "^18.2.14",
    "@types/react-slider": "^1.3.1",
    "@types/w3c-image-capture": "^1.0.7",
    "@types/webpack-env": "^1.18.1",
    "copyfiles": "^2.4.1",
    "node-sass": "^9.0.0",
    "rimraf": "^5.0.1",
    "tsc-alias": "^1.8.7",
    "typescript": "^5.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KrimsonKing1337/activeBook-core.git"
  },
  "author": "Oleg Shilov Jr",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/KrimsonKing1337/activeBook-core/issues"
  },
  "homepage": "https://github.com/KrimsonKing1337/activeBook-core#readme"
}
