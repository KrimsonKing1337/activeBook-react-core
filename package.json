    "@fortawesome/react-fontawesome": "^0.2.0",
    "@reduxjs/toolkit": "^1.9.5",
    "classnames": "^2.3.2",
    "encrypt-storage": "^2.12.16",
    "hammerjs": "^2.0.8",
    "howler": "^2.2.3",
    "modern-css-reset": "^1.4.0",
    "moment": "^2.29.4",
    "nanoid": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^7.2.9",
    "react-router": "^6.15.0",
    "react-router-dom": "^6.15.0",
    "react-slider": "^2.0.6",
    "react-toastify": "^9.1.3",
    "redux-first-history": "^5.1.1",
    "redux-saga": "^1.2.3"
  },
  "devDependencies": {
    "@types/classnames": "^2.3.1",
    "@types/hammerjs": "^2.0.41",
    "@types/howler": "^2.2.8",
    "@types/node": "^20.5.6",
